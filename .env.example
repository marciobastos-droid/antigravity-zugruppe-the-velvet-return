# Base44 Configuration
# Copy this file to .env.local and fill in your values

# Your Base44 App ID (get this from your Base44 dashboard)
VITE_BASE44_APP_ID=your_app_id_here

# Base44 Backend URL (e.g., https://api.base44.com)
VITE_BASE44_BACKEND_URL=your_backend_url_here

# Optional: Legacy SDK imports flag
BASE44_LEGACY_SDK_IMPORTS=false

# Google OAuth Configuration
# Get your Client ID from Google Cloud Console (https://console.cloud.google.com)
VITE_GOOGLE_CLIENT_ID=your_google_client_id_here
